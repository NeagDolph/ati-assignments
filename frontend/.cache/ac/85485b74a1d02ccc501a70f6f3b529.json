ms({...indexLoadPayload.data, userid: indexLoadPayload.userid});\n        return;\n      }\n      if (this.$store.state.loggedin) {\n        this.joinRooms(this.$store.state.user);\n        return;\n      }\n\n      this.getUserData().then(data => {\n        this.joinRooms(data)\n        this.$store.commit(\"mutate\", [\"loggedin\", true])\n      })\n      .catch(err => {\n        if (err.response.status == 403) {\n        }\n      })\n    })\n    .on('connect_error', error => {\n      console.log('%c Socket cannot connect to backend API!', 'background: #222; color: #ff6961; font-size: 14px;');\n    })\n  }\n})\n"},"lineCount":null}},"error":null,"hash":"fda443f849c8ef578447c5ffbf58406e","cacheData":{"env":{}}}