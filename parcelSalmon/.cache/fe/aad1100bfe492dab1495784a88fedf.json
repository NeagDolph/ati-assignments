lumn":13}},{"name":"area","generated":{"line":61,"column":34},"source":"mixins/mixins.js","original":{"line":52,"column":16}},{"generated":{"line":61,"column":38},"source":"mixins/mixins.js","original":{"line":7,"column":13}},{"name":"val","generated":{"line":61,"column":40},"source":"mixins/mixins.js","original":{"line":52,"column":22}},{"generated":{"line":61,"column":43},"source":"mixins/mixins.js","original":{"line":7,"column":13}},{"generated":{"line":61,"column":45},"source":"mixins/mixins.js","original":{"line":52,"column":27}},{"name":"app","generated":{"line":62,"column":0},"source":"mixins/mixins.js","original":{"line":53,"column":8}},{"name":"global","generated":{"line":62,"column":16},"source":"mixins/mixins.js","original":{"line":53,"column":12}},{"generated":{"line":62,"column":22},"source":"mixins/mixins.js","original":{"line":53,"column":8}},{"name":"area","generated":{"line":62,"column":23},"source":"mixins/mixins.js","original":{"line":53,"column":19}},{"generated":{"line":62,"column":27},"source":"mixins/mixins.js","original":{"line":53,"column":8}},{"name":"val","generated":{"line":62,"column":31},"source":"mixins/mixins.js","original":{"line":53,"column":27}},{"generated":{"line":62,"column":34},"source":"mixins/mixins.js","original":{"line":53,"column":8}},{"generated":{"line":63,"column":0},"source":"mixins/mixins.js","original":{"line":54,"column":7}},{"generated":{"line":64,"column":0},"source":"mixins/mixins.js","original":{"line":7,"column":13}},{"generated":{"line":65,"column":0},"source":"mixins/mixins.js","original":{"line":6,"column":10}},{"generated":{"line":65,"column":1},"source":"mixins/mixins.js","original":{"line":6,"column":0}}],"sources":{"mixins/mixins.js":"import axios from 'axios';\nimport Vue from \"vue\";\nimport { apiurl } from \"../js/globals\";\nimport { app } from '../main';\n\nVue.mixin({\n    methods: {\n      usereditmodal(state, user=false) {\n        app.editState = state;\n        if (user) app.editSelect = user\n      },\n      editUserClasses(user, idx, userindex) {\n        app.sharedData.users[userindex].oldclasses = user.classes\n        let classes = user.classes.split(\"\")\n        classes[idx] = classes[idx] == \"0\" ? \"1\" : (classes[idx] == \"1\" ? \"0\" : \"1\")\n        classes = classes.join(\"\")\n        app.sharedData.users[userindex].classes = classes\n  \n        axios\n          .post(apiurl.classes, { class: idx, userid: user.userid, new: classes[idx] })\n          .then(() => {\n            axios.post(apiurl.data);\n          })\n          .catch(error => {\n            console.log(error.response)\n            console.log(\"error: \", app.sharedData.users[userindex].oldclasses, user.classes, classes)\n            app.sharedData.users[userindex].classes = app.sharedData.users[userindex].oldclasses\n          });\n      },\n      addComment(user, idx, usercomment) {\n        axios\n        .post(apiurl.comment, { class: idx, userid: user.userid, \"comment\": usercomment})\n        .then((data) => {\n          return \"PEE\"\n        })\n        .catch(() => {\n          console.log(\"EE\")\n          return \"PEE\"\n        })\n      },\n      requestComment(user, idx) {\n        axios\n          .post(apiurl.getcomment, { class: idx, userid: user.userid })\n          .then((data) => {\n            return \"PEE\"\n            // return data\n          })\n          .catch((e) => {\n            console.log(\"Error requesting comment\", e)\n          })\n      },\n      setGlobal(area, val) {\n        app.global[area] = val\n      }\n    }\n})"},"lineCount":null}},"error":null,"hash":"982ebb98daf35be536821d69da4c5266","cacheData":{"env":{}}}