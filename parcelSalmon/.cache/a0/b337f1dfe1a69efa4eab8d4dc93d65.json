,"original":{"line":28,"column":0},"generated":{"line":1,"column":505}}],"sources":{"components/commentmodal.vue":"<template>\n  <div class=\"modal micromodal-slide\" id=\"modal-useredit\" aria-hidden=\"true\">\n    <div class=\"modal__overlay\" tabindex=\"-1\" data-micromodal-close>\n      <div class=\"modal__container\" role=\"dialog\" aria-modal=\"true\" aria-labelledby=\"modal-1-title\">\n        <header class=\"modal__header\">\n          <h2 class=\"modal__title\" id=\"modal-1-title\">{{userdata.name}}</h2>\n          <button class=\"modal__close\" aria-label=\"Close modal\" data-micromodal-close></button>\n        </header>\n        <main class=\"modal__content row\" id=\"modal-1-content\">\n          <div class=\"class mx-auto\" :key=\"shortnames[index]\">\n            <div v-if=\"classStatus !== '2'\">\n              <h3 class=\"title\">{{classnames[index]}}</h3>\n              <commentarea class=\"col-11 mx-auto\" :user=\"userdata\" :classidx=\"index\"></commentarea>\n            </div>\n          </div>\n        </main>\n        <footer class=\"modal__footer\">\n        </footer>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport commentarea from './commentarea.vue'\nimport { sharedData, classnames } from './../js/globals.js'\n\nexport default {\n  props: [\"userdata\", \"classes\", \"users\", \"tclasses\"],\n  components: {\n    commentarea\n  },\n  data() {\n    return {\n      shortnames: sharedData.shortnames,\n      classnames\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../css/settings.scss\";\n\n.modal__content {\n  margin-top: 2rem;\n  margin-bottom: 2rem;\n  line-height: 1.5;\n  color: $color_2;\n\n  .class {\n    margin-bottom: 20px;\n\n    .title {\n      margin-top: 0;\n      margin-bottom: 0;\n      font-weight: 600;\n      font-size: 1.25rem;\n      line-height: 1.25;\n      text-align: center;\n      color: $color_1;\n    }\n  }\n\n}\n\n@keyframes mmfadeIn {\n  from {\n    opacity: 0;\n    transform: translateY(15%);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n@keyframes mmfadeOut {\n  from {\n    opacity: 1;\n    transform: translateY(0);\n  }\n  to {\n    opacity: 0;\n    transform: translateY(-10%);\n  }\n}\n\n.modal {\n  font-family: 'Fanta';\n}\n.modal__overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: rgba(0, 0, 0, 0.6);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.modal__container {\n  background-color: $background;\n  padding: 30px;\n  max-width: 500px;\n  max-height: 100vh;\n  border-radius: 4px;\n  width: 350px;\n  overflow-y: auto;\n  box-sizing: border-box;\n}\n.modal__header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  .modal__close {\n    &:before {\n      content: \"\\2715\";\n    }\n  }\n}\n.modal__title {\n  margin-top: 0;\n  margin-bottom: 0;\n  font-weight: 600;\n  font-size: 1.25rem;\n  line-height: 1.25;\n  color: $color_1;\n  box-sizing: border-box;\n}\n.modal__close {\n  background: transparent;\n  border: 0;\n}\n.modal__btn {\n  font-size: 0.875rem;\n  padding-left: 1rem;\n  padding-right: 1rem;\n  padding-top: 0.5rem;\n  padding-bottom: 0.5rem;\n  background-color: $background;\n  color: $color_2;\n  border-radius: 0.25rem;\n  border-style: none;\n  border-width: 0;\n  cursor: pointer;\n  -webkit-appearance: button;\n  text-transform: none;\n  overflow: visible;\n  line-height: 1.15;\n  margin: 0;\n  will-change: transform;\n  -moz-osx-font-smoothing: grayscale;\n  -webkit-backface-visibility: hidden;\n  backface-visibility: hidden;\n  -webkit-transform: translateZ(0);\n  transform: translateZ(0);\n  transition: -webkit-transform 0.25s ease-out;\n  transition: transform 0.25s ease-out;\n  transition: transform 0.25s ease-out, -webkit-transform 0.25s ease-out;\n  &:focus {\n    -webkit-transform: scale(1.05);\n    transform: scale(1.05);\n  }\n  &:hover {\n    -webkit-transform: scale(1.05);\n    transform: scale(1.05);\n  }\n}\n.modal__btn-primary {\n  background-color: $background;\n  color: $color_3;\n}\n.micromodal-slide {\n  display: none;\n  .modal__container {\n    will-change: transform;\n  }\n  .modal__overlay {\n    will-change: transform;\n  }\n}\n</style>"},"lineCount":null}},"error":null,"hash":"cf87c7855738cb84249803a0d6aeb481","cacheData":{"env":{}}}